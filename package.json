{
  "name": "polar-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "lerna run compile",
    "publish": "lerna exec --no-private -- npm publish",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist-version": "lerna --no-git-tag-version version patch",
    "dist-prepare": "./sbin/mgit clean -f && ./sbin/mgit reset --hard HEAD && ./sbin/mgit pull",
    "dist-publish": "npm run-script purge-node-modules && lerna bootstrap && lerna run compile && lerna run dist && lerna exec --no-private -- npm publish",
    "dist-release": "npm run-script dist-publish && lerna run dist-release",
    "lerna-outdated": "lerna exec --concurrency=1 --parallel=false --no-bail -- 'pwd && npm outdated'",
    "purge-node-modules": "lerna exec --concurrency=1 --parallel=false --no-bail -- 'pwd && rm -rf node_modules'",
    "find-broken-imports": "find . -type f -name \"*.ts\" ! -path '*/node_modules/*' |xargs grep import |grep -E \"[\\\"'](\\.\\./)+polar\"\n"
  },
  "deprecated": {
  },
  "keywords": [],
