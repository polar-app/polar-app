{
  "name": "polar-web-extension",
  "version": "polar.latest",
  "description": "https://github.com/yeoman/generator-chrome-extension",
  "scripts": {
    "_test": "mocha --timeout 20000 --exit 'src/**/*Test.js'",
    "test": "RESULT=\"$(find . -name '**Test.js' -o -name '**TestN.js' -o -name '**TestNK.js' -not -path 'node_modules/*')\" && if [ -z \"$RESULT\" ]; then echo 'No tests'; else yarn run mocha; fi;",
    "clean": "rm -rf .webpack-cache-loader && rm -rf .terser-webpack-plugin && rm -rf dist && rm -rf polar-web-extension.zip",
    "compile": "RESULT=\"$(find . -name '*.ts' -o -name '*.tsx' -not -path './node_modules/*' -not -name '*.d.ts*')\" && if [ -z \"$RESULT\" ]; then echo 'Nothing to Compile'; else yarn run tsc; fi;",
    "webpack": "npx webpack --config=webpack.config.js",
    "dist": "unset NODE_ENV && unset WEBPACK_TARGET && yarn run clean && yarn run webpack && ./dist.sh",
    "mocha": "mocha -p --jobs=1 --timeout 60000 --exit './{,!(node_modules)/**}/*Test.js' './{,!(node_modules)/**}/*TestN.js' './{,!(node_modules)/**}/*TestNK.js'",
    "eslint": "eslint -c ./.eslintrc.json .",
    "eslintfix": "eslint -c ./.eslintrc.json . --fix",
    "tsc": "tsc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.13.0",
    "@material-ui/core": "=4.11.3",
    "@material-ui/icons": "=4.11.2",
    "page-metadata-parser": "=1.1.4",
    "polar-assets": "polar.latest",
    "polar-bookshelf": "polar.latest",
    "polar-content-capture": "polar.latest",
    "polar-epub-generator": "polar.latest",
    "polar-firebase-browser": "polar.latest",
    "polar-html": "polar.latest",
    "polar-react": "polar.latest",
    "polar-shared": "polar.latest",
    "polar-web-extension-api": "polar.latest",
    "readability": "0.2.0-polar.2"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/chrome": "0.0.114",
    "@types/mocha": "=9.0.0",
    "@types/node": "=14.14.35",
    "@types/page-metadata-parser": "polar.latest",
    "@types/react": "=16.9.53",
    "@types/react-dom": "=16.9.8",
    "chai": "4.2.0",
    "mocha": "=9.1.1",
    "polar-eslint": "polar.latest",
    "polar-karma": "polar.latest",
    "polar-mocha": "polar.latest",
    "polar-typescript": "polar.latest",
    "polar-webpack": "polar.latest",
    "webpack": "=5.26.3",
    "webpack-cli": "=4.5.0"
  },
  "files": [
    "package.json"
  ]
}
