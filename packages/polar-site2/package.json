{
  "name": "polar",
  "private": true,
  "description": "General information, support, and downloads for the polar app",
  "version": "2.0.85",
  "author": "Will Kendall / Jonathan Graeupner / Myles <wpk235@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "clean-dev": "gatsby clean && gatsby develop",
    "dist": "gatsby clean && gatsby build",
    "dist-release": "npm run-script dist && firebase deploy",
    "build-deploy-dev": "gatsby clean && gatsby build && firebase deploy && gatsby develop",
    "prune-dev": "npm prune && gatsby develop",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve --port=8000",
    "clean": "gatsby clean",
    "get-release": "curl -s https://api.github.com/repos/burtonator/polar-bookshelf/releases/latest |jq -r .name | xargs printf \"export const POLAR_RELEASE = '%s';\n\" > src/components/release.ts",
    "prebuild": "npm run-script get-release"
  },
  "dependencies": {
    "@material-ui/core": "=4.11.0",
    "@material-ui/icons": "=4.9.1",
    "@material-ui/styles": "^4.10.0",
    "@types/node": "12.12.32",
    "@types/react": "=16.9.53",
    "@types/react-helmet": "=6.1.0",
    "gatsby": "=2.27.1",
    "gatsby-image": "=2.6.0",
    "gatsby-plugin-breakpoints": "=1.2.6",
    "gatsby-plugin-google-analytics": "=2.6.0",
    "gatsby-plugin-manifest": "=2.7.0",
    "gatsby-plugin-material-ui": "=2.1.10",
    "gatsby-plugin-react-helmet": "=3.5.0",
    "gatsby-plugin-sharp": "=2.9.0",
    "gatsby-plugin-sitemap": "=2.7.0",
    "gatsby-remark-images": "=3.6.0",
    "gatsby-source-filesystem": "=2.6.1",
    "gatsby-theme-material-ui": "=1.0.13",
    "gatsby-transformer-remark": "=2.11.0",
    "gatsby-transformer-sharp": "=2.7.0",
    "ink": "=3.0.8",
    "ink-box": "=1.0.0",
    "pngquant-bin": "=6.0.0",
    "polar-bookshelf": "^2.0.85",
    "polar-shared": "^2.0.85",
    "react": "=16.14.0",
    "react-dom": "=16.14.0",
    "react-helmet": "=6.1.0",
    "react-reveal": "=1.2.2",
    "typeface-roboto": "=1.1.13"
  },
  "devDependencies": {
    "prettier": "2.1.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a"
}
