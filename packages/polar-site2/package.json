{
  "name": "polar-site2",
  "description": "General information, support, and downloads for the polar app",
  "version": "0.0.0",
  "author": "Will Kendall / Jonathan Graeupner / Myles <wpk235@gmail.com>",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "clean-dev": "gatsby clean && gatsby develop",
    "dist": "yarn run clean && gatsby build",
    "dist-release": "yarn run dist && firebase deploy",
    "build-deploy-dev": "gatsby clean && gatsby build && firebase deploy && gatsby develop",
    "prune-dev": "npm prune && gatsby develop",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve --port=8000",
    "clean": "gatsby clean && find src/pages/images -name '*.jsx' -exec rm '{}' ';'",
    "get-release": "curl -s https://api.github.com/repos/burtonator/polar-bookshelf/releases/latest |jq -r .name | xargs printf \"export const POLAR_RELEASE = '%s';\n\" > src/components/release.ts",
    "prebuild": "yarn run get-release"
  },
  "dependencies": {
    "@material-ui/core": "=4.12.3",
    "@material-ui/icons": "=4.11.2",
    "@material-ui/styles": "=4.11.4",
    "@types/node": "=14.17.9",
    "@types/react": "=16.9.53",
    "@types/react-helmet": "=6.1.0",
    "gatsby": "=3.14.2",
    "gatsby-image": "=3.11.0",
    "gatsby-plugin-amplitude-analytics": "=3.2.0",
    "gatsby-plugin-breakpoints": "=1.3.5",
    "gatsby-plugin-google-analytics": "=3.14.0",
    "gatsby-plugin-manifest": "=3.14.0",
    "gatsby-plugin-material-ui": "=4.0.3",
    "gatsby-plugin-react-helmet": "=4.14.0",
    "gatsby-plugin-sharp": "=3.14.1",
    "gatsby-plugin-sitemap": "=4.10.0",
    "gatsby-remark-images": "=5.11.0",
    "gatsby-source-filesystem": "=3.14.0",
    "gatsby-theme-material-ui": "=5.0.0",
    "gatsby-transformer-remark": "=4.11.0",
    "gatsby-transformer-sharp": "=3.14.0",
    "ink": "=3.2.0",
    "ink-box": "=1.0.0",
    "pngquant-bin": "=6.0.0",
    "polar-bookshelf": "0.0.0",
    "polar-shared": "0.0.0",
    "react": "=17.0.2",
    "react-dom": "=17.0.2",
    "react-helmet": "=6.1.0",
    "react-reveal": "=1.2.2",
    "typeface-roboto": "=1.1.13",
    "@babel/plugin-transform-typescript": "=7.15.8",
    "babel-preset-gatsby": "=1.14.0"
  },
  "devDependencies": {
    "prettier": "=2.3.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a"
}
