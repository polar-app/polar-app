{
  "name": "polar-create-module",
  "version": "2.1.107",
  "description": "A module to create polar-modules",
  "scripts": {
    "test": "if [ -z \"$(find . -name '*Test.js' -o -name '*TestN.js' -o -name '*TestNK.js')\" ]; then echo 'No tests'; else yarn run mocha; fi;",
    "mocha": "mocha -p --timeout 60000 --exit './{,!(node_modules)/**}/*Test.js' './{,!(node_modules)/**}/*TestN.js' './{,!(node_modules)/**}/*TestNK.js'",
    "eslint": "eslint -c ./.eslintrc.json .",
    "eslintfix": "eslint -c ./.eslintrc.json . --fix",
    "compile": "tsc",
    "build": "echo '#!/usr/bin/env node' > ./bin/polar-create-module && cat src/CreateModule.js >> ./bin/polar-create-module",
    "publish": "yarn run build"
  },
  "bin": {
    "polar-create-module": "./bin/polar-create-module"
  },
  "dependencies": {
    "polar-shared": "^2.1.107"
  },
  "author": "Polar",
  "license": "ISC",
  "devDependencies": {
    "polar-eslint": "^2.1.107",
    "polar-typescript": "^2.1.107"
  }
}