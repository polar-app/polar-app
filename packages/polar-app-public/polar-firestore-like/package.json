{
  "name": "polar-firestore-like",
  "version": "2.1.107",
  "description": "Firestore interfaces so that we can use both the client and admin SDK and mock the firestore interfaces.",
  "main": "index.js",
  "scripts": {
    "_test": "mocha --timeout 20000 --exit 'src/**/*Test.js'",
    "compile": "tsc",
    "test": "if [ -z \"$(find . -name '*Test.js' -o -name '*TestN.js' -o -name '*TestNK.js')\" ]; then echo 'No tests'; else yarn run mocha; fi;",
    "mocha": "mocha -p --jobs=1 --timeout 60000 --exit './{,!(node_modules)/**}/*Test.js' './{,!(node_modules)/**}/*TestN.js' './{,!(node_modules)/**}/*TestNK.js'",
    "eslint": "eslint -c ./.eslintrc.json .",
    "eslintfix": "eslint -c ./.eslintrc.json . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/mocha": "=9.0.0",
    "chai": "4.2.0",
    "mocha": "=9.1.1",
    "polar-test": "^2.1.107",
    "tslint": "6.1.2",
    "typescript": "=4.2.3",
    "polar-eslint": "^2.1.107",
    "polar-typescript": "^2.1.107"
  },
  "dependencies": {
    "polar-shared": "^2.1.107"
  },
  "peerDependencies": {
    "firebase": "=8.8.1",
    "firebase-admin": "=9.11.0"
  }
}