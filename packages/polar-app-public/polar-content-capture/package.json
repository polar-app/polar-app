    "chai": "4.2.0",
    "jsdom": "=16.7.0",
    "mocha": "=9.1.1",
    "polar-mocha": "^2.1.107",
    "polar-test": "^2.1.107",
    "tslint": "6.1.2",
    "typescript": "=4.2.3",
    "wait-for-expect": "3.0.1",
    "polar-eslint": "^2.1.107",
    "polar-typescript": "^2.1.107"
  },
  "dependencies": {
    "jszip": "=3.5.0",
