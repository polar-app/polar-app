{
  "name": "polar-test",
  "version": "2.1.107",
  "description": "",
  "main": "index.js",
  "scripts": {
    "_test": "mocha --timeout 20000 --exit 'src/**/*Test.js'",
    "test": "RESULT=\"$(find . -name '**Test.js' -not -path 'node_modules/*')\" && if [ -z \"$RESULT\" ]; then echo 'No tests'; else yarn run mocha; fi;",
    "compile": "RESULT=\"$(find -name '*.ts' -not -path './node_modules/*' -not -name '*.d.ts*')\" && if [ -z \"$RESULT\" ]; then echo 'Nothing to Compile'; else yarn run tsc; fi;",
    "mocha": "mocha --timeout 20000 --exit './{,!(node_modules)/**}/*Test.js'",
    "eslint": "eslint -c ./.eslintrc.json .",
    "eslintfix": "eslint -c ./.eslintrc.json . --fix",
    "tsc": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/diff": "4.0.2",
    "tslint": "6.1.2",
    "typescript": "=4.2.3",
    "polar-eslint": "^2.1.107",
    "polar-typescript": "^2.1.107"
  },
  "dependencies": {
    "@types/node": "=14.14.35",
    "chai": "4.2.0",
    "diff": "4.0.2",
    "mocha": "=9.0.0"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "package.json"
  ]
}