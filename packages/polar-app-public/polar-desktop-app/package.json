{
  "name": "polar-desktop-app",
  "version": "2.0.0-beta.24",
  "description": "Polar",
  "main": "./main-bundle.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "publish-linux": "npx webpack && electron-builder --config=electron-builder.yml --linux appimage deb tar.gz --publish always",
    "publish-macos": "npx webpack && electron-builder --config=electron-builder.yml --macos dmg zip --publish always",
    "publish-win": "npx webpack && ./build/dist-win.sh"
  },
  "repository": "https://github.com/burtonator/polar-app-public",
  "keywords": [
    "Electron",
    "PDF",
    "viewer",
    "PDF.js",
    "ebook",
    "annotations"
  ],
  "author": {
    "name": "contributors",
    "email": "noone@example.com"
  },
  "contributors": [
    {
      "name": "Kevin Burton"
    }
  ],
  "build": {
    "appId": "io.inputneuron/polar",
    "mac": {
      "category": "Books"
    }
  },
  "license": "GPL-3.0-only",
  "devDependencies": {
    "cache-loader": "4.1.0",
    "electron": "=8.4.0",
    "electron-builder": "~22.7.0",
    "electron-notarize": "1.0.0",
    "fork-ts-checker-webpack-plugin": "=5.0.7",
    "mocha": "8.0.1",
    "null-loader": "=4.0.0",
    "source-map-support": "=0.5.19",
    "thread-loader": "=2.1.3",
    "ts-loader": "=8.0.0",
    "tslint": "6.1.2",
    "typescript": "3.9.5",
    "webpack": "=4.43.0",
    "webpack-cli": "=3.3.12"
  },
  "dependencies": {
    "polar-shared": "^2.0.0-beta.24"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/polar-bookshelf"
  }
}
