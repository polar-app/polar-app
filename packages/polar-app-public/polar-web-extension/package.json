{
  "name": "polar-web-extension",
  "version": "2.1.107",
  "description": "https://github.com/yeoman/generator-chrome-extension",
  "scripts": {
    "_test": "mocha --timeout 20000 --exit 'src/**/*Test.js'",
    "test": "if [ -z \"$(find src -name '**Test.js')\" ]; then echo 'No tests'; else yarn run mocha; fi;",
    "clean": "rm -rf .webpack-cache-loader && rm -rf .terser-webpack-plugin && rm -rf dist && rm -rf polar-web-extension.zip",
    "compile": "tsc",
    "webpack": "npx webpack --config=webpack.config.js",
    "dist": "unset NODE_ENV && unset WEBPACK_TARGET && yarn run clean && yarn run webpack && ./dist.sh",
    "mocha": "mocha --timeout 20000 --exit './{,!(node_modules)/**}/*Test.js'",
    "eslint": "eslint -c ./.eslintrc.json .",
    "eslintfix": "eslint -c ./.eslintrc.json . --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.13.0",
    "@material-ui/core": "=4.11.3",
    "@material-ui/icons": "=4.11.2",
    "page-metadata-parser": "=1.1.4",
    "polar-assets": "^2.1.107",
    "polar-bookshelf": "^2.1.107",
    "polar-content-capture": "^2.1.107",
    "polar-epub-generator": "^2.1.107",
    "polar-firebase-browser": "^2.1.107",
    "polar-html": "^2.1.107",
    "polar-react": "^2.1.107",
    "polar-shared": "^2.1.107",
    "polar-web-extension-api": "^2.1.107",
    "readability": "0.2.0-polar.2"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/chrome": "0.0.114",
    "@types/mocha": "=8.2.2",
    "@types/node": "=14.14.35",
    "@types/page-metadata-parser": "^2.1.107",
    "@types/react": "=16.9.53",
    "@types/react-dom": "=16.9.8",
    "chai": "4.2.0",
    "mocha": "=9.0.0",
    "polar-karma": "^2.1.107",
    "polar-mocha": "^2.1.107",
    "polar-webpack": "^2.1.107",
    "tslint": "6.1.2",
    "typescript": "=4.2.3",
    "webpack": "=5.26.3",
    "webpack-cli": "=4.5.0",
    "polar-eslint": "^2.1.107",
    "polar-typescript": "^2.1.107"
  },
  "files": [
    "package.json"
  ]
}