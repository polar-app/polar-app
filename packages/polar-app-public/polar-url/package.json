{
  "name": "polar-url",
  "version": "2.1.107",
  "description": "URL parsing framework that's portable to Node 8, 10, etc.",
  "scripts": {
    "test": "RESULT=\"$(find . -name '**Test.js' -not -path 'node_modules/*')\" && if [ -z \"$RESULT\" ]; then echo 'No tests'; else yarn run mocha; fi;",
    "compile": "RESULT=\"$(find -name '*.ts' -not -path './node_modules/*' -not -name '*.d.ts*')\" && if [ -z \"$RESULT\" ]; then echo 'Nothing to Compile'; else yarn run tsc; fi;",
    "mocha": "mocha --timeout 20000 --exit './{,!(node_modules)/**}/*Test.js'",
    "eslint": "eslint -c ./.eslintrc.json .",
    "eslintfix": "eslint -c ./.eslintrc.json . --fix",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/burtonator/polar-shared.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/burtonator/polar-shared/issues"
  },
  "homepage": "https://github.com/burtonator/polar-shared#readme",
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/mocha": "=8.2.2",
    "chai": "4.2.0",
    "mocha": "=9.0.0",
    "polar-test": "^2.1.107",
    "tslint": "6.1.2",
    "typescript": "=4.2.3",
    "polar-eslint": "^2.1.107",
    "polar-typescript": "^2.1.107"
  },
  "dependencies": {
    "@types/whatwg-url": "^6.4.0",
    "whatwg-url": "^8.0.0"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "package.json"
  ]
}