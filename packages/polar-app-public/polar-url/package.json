{
  "name": "polar-url",
  "version": "2.1.107",
  "description": "URL parsing framework that's portable to Node 8, 10, etc.",
  "scripts": {
    "test": "if [ -z \"$(find . -name '*Test.js' -o -name '*TestN.js' -o -name '*TestNK.js')\" ]; then echo 'No tests'; else yarn run mocha; fi;",
    "compile": "tsc",
    "mocha": "mocha -p --jobs=1 --timeout 60000 --exit './{,!(node_modules)/**}/*Test.js' './{,!(node_modules)/**}/*TestN.js' './{,!(node_modules)/**}/*TestNK.js'",
    "eslint": "eslint -c ./.eslintrc.json .",
    "eslintfix": "eslint -c ./.eslintrc.json . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/burtonator/polar-shared.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/burtonator/polar-shared/issues"
  },
  "homepage": "https://github.com/burtonator/polar-shared#readme",
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/mocha": "=9.0.0",
    "chai": "4.2.0",
    "mocha": "=9.1.1",
    "polar-test": "^2.1.107",
    "tslint": "6.1.2",
    "typescript": "=4.2.3",
    "polar-eslint": "^2.1.107",
    "polar-typescript": "^2.1.107"
  },
  "dependencies": {
    "@types/whatwg-url": "^6.4.0",
    "whatwg-url": "^8.0.0"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "package.json"
  ]
}