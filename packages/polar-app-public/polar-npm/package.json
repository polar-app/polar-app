{
  "name": "polar-npm",
  "version": "2.1.107",
  "description": "Basic internal NPM commands which extend NPM functionality",
  "main": "index.js",
  "scripts": {
    "test": "if [ -z $(find src -name '**Test.js') ]; then echo 'No tests'; else yarn run mocha; fi;",
    "compile": "tsc",
    "mocha": "mocha --timeout 20000 --exit **/**/*Test.js",
    "eslint": "eslint -c ./.eslintrc.json .",
    "eslintfix": "eslint -c ./.eslintrc.json . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bin": {
    "hello": "src/hello.js",
    "npm-upgrade-pkg": "src/npm-upgrade-pkg.js",
    "npm-stagnant": "src/npm-stagnant.js"
  },
  "devDependencies": {
    "@types/node": "=14.14.35",
    "@types/pdfjs-dist": "2.1.71-polar.2",
    "tslint": "6.1.2",
    "typescript": "=4.2.3",
    "polar-eslint": "^2.1.107",
    "polar-typescript": "^2.1.107"
  },
  "dependencies": {
    "polar-shared": "^2.1.107"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "package.json"
  ]
}