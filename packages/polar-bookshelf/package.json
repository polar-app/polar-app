{
  "name": "polar-bookshelf",
  "version": "0.0.0",
  "description": "Polar",
  "scripts": {
    "testit": "echo ./dist/polar-bookshelf_${npm_package_version}_amd64.snap",
    "clean": "rm -rf web/dist && rm -rf dist/public && rm -rf .webpack-cache-loader && rm -rf .terser-webpack-plugin && rm -rf tsconfig.tsbuildinfo",
    "build-webapp": "yarn run clean && yarn run eslint && npx tsc && export NODE_ENV=production && export WEBPACK_TARGET=web && yarn run webpack",
    "setup-dist-appx": "cp web/js/dist_config/AppXDistConfig.ts web/js/dist_config/DistConfig.ts",
    "publish-linux-snapcraft": "snapcraft push --release stable ./dist/polar-bookshelf-${npm_package_version}-amd64.snap",
    "publish-webapp": "yarn run build-webapp && firebase use polar-32b0f && ./scripts/firebase-set-hosting && firebase deploy",
    "publish-win": "./scripts/dist-win.sh",
    "__dist-linux": "yarn run setup-dist-default && yarn run compile && yarn run publish-linux && yarn run publish-linux-snapcraft",
    "dist-webapp": "yarn run setup-dist-default && yarn run compile && yarn run publish-webapp",
    "dist": "yarn run build-webapp",
    "dist-prepare": "",
    "compile": "RESULT=\"$(find . -name '*.ts' -o -name '*.tsx' -not -path './node_modules/*' -not -name '*.d.ts*')\" && if [ -z \"$RESULT\" ]; then echo 'Nothing to Compile'; else yarn run tsc; fi;",
    "compile-watch": "tsc --watch",
    "test-mocha": "npx mocha --exit --log-timer-events --recursive 'web/js/**/*Test.js'",
    "_test": "./scripts/test-runner.sh",
    "___test": "mocha --timeout 20000 --exit 'web/js/**/*Test.js' 'apps/**/*Test.js'",
    "test": "RESULT=\"$(find . -name '**Test.js' -o -name '**TestN.js' -o -name '**TestNK.js' -not -path 'node_modules/*')\" && if [ -z \"$RESULT\" ]; then echo 'No tests'; else yarn run mocha; fi;",
    "__test": "scripts/test.sh",
    "eslint": "eslint -c ./.eslintrc.json . --no-error-on-unmatched-pattern",
    "webpack": "rm -rf dist/public && rm -rf .webpack-cache-loader && rm -rf .terser-webpack-plugin/ && mkdir -p dist/public && npx --max_old_space_size=4096 webpack --config=webpack.config.js && pwd && ls -al dist/public && echo 'Webpack built successfully'",
    "webpack-serve": "node --max_old_space_size=4096 ../../node_modules/.bin/webpack serve --host=0.0.0.0",
    "find-unhandled-promises": "yarn run tslint | grep 'Promises must be handled appropriately' | grep -v 'web/test' ; true",
    "serve": "node scripts/serve.js",
    "karma": "echo no karma",
    "karma:dev": "../../node_modules/karma/bin/karma start --browsers Chrome --singleRun false",
    "mocha": "mocha -p --jobs=1 --timeout 60000 --exit './{,!(node_modules)/**}/*Test.js' './{,!(node_modules)/**}/*TestN.js' './{,!(node_modules)/**}/*TestNK.js'",
    "eslintfix": "eslint -c ./.eslintrc.json . --fix",
    "tsc": "tsc",
    "watch": "RESULT=\"$(find . -name '*.ts' -o -name '*.tsx' -not -path './node_modules/*' -not -name '*.d.ts*')\" && if [ -z \"$RESULT\" ]; then echo 'Nothing to Compile'; else yarn run tscwatch; fi;",
    "tscwatch": "tsc --watch",
    "citest": "RESULT=\"$(find . -name '**Test.js' -o -name '**TestN.js' -o -name '**TestNK.js' -not -path 'node_modules/*')\" && if [ -z \"$RESULT\" ]; then echo 'No tests'; else yarn run mocha; fi;",
    "cimocha": "mocha -p --reporter xunit --reporter-option output=test_results.xml --jobs=1 --timeout 60000 --exit './{,!(node_modules)/**}/*Test.js' './{,!(node_modules)/**}/*TestN.js' './{,!(node_modules)/**}/*TestNK.js'",
    "cieslint": "eslint -c ./.eslintrc.json -f compact . --no-error-on-unmatched-pattern"
  },
  "repository": "https://github.com/burtonator/polar-bookshelf",
  "keywords": [
    "PDF",
    "viewer",
    "PDF.js",
