    "@types/chai": "4.2.11",
    "@types/mocha": "=8.2.2",
    "chai": "4.2.0",
    "mocha": "=9.0.0",
    "polar-firebase-admin": "^2.1.107",
    "polar-firebase-browser": "^2.1.107",
    "polar-firebase-test": "^2.1.107",
    "polar-test": "^2.1.107",
    "tslint": "6.1.2",
    "typescript": "=4.2.3",
    "polar-eslint": "^2.1.107",
    "polar-typescript": "^2.1.107"
  },
  "dependencies": {
    "polar-blocks": "^2.1.107",
