{
  "name": "polar-block-permissions",
  "version": "2.1.107",
  "description": "Code to connect to firebase as an admin user",
  "main": "index.js",
  "scripts": {
    "_test": "mocha --timeout 20000 --exit 'src/**/*Test.js'",
    "test": "RESULT=\"$(find . -name '**Test.js' -not -path 'node_modules/*')\" && if [ -z \"$RESULT\" ]; then echo 'No tests'; else yarn run mocha; fi;",
    "compile": "RESULT=\"$(find -name '*.ts' -o -name '*.tsx' -not -path './node_modules/*' -not -name '*.d.ts*')\" && if [ -z \"$RESULT\" ]; then echo 'Nothing to Compile'; else yarn run tsc; fi;",
    "dist": "echo noop",
    "mocha": "mocha --timeout 20000 --exit -p './{,!(node_modules)/**}/*Test.js'",
    "eslint": "eslint -c ./.eslintrc.json .",
    "eslintfix": "eslint -c ./.eslintrc.json . --fix",
    "tsc": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
