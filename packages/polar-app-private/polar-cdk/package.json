{
  "name": "polar-cdk",
  "version": "2.1.107",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "test": "if [ -z \"$(find . -name '*Test.js' -o -name '*TestN.js' -o -name '*TestNK.js')\" ]; then echo 'No tests'; else yarn run mocha; fi;",
    "mocha": "mocha -p --timeout 60000 --exit './{,!(node_modules)/**}/*Test.js' './{,!(node_modules)/**}/*TestN.js' './{,!(node_modules)/**}/*TestNK.js'",
    "eslint": "eslint -c ./.eslintrc.json .",
    "eslintfix": "eslint -c ./.eslintrc.json . --fix",
    "compile": "tsc"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.117.0",
    "@google-cloud/firestore": "=4.13.2",
    "@types/node": "=14.14.35",
    "aws-cdk": "^1.117.0",
    "esbuild": "^0.12.19",
    "firebase-admin": "=9.11.0",
    "ts-node": "^10.2.0",
    "typescript": "=4.2.3",
    "polar-eslint": "^2.1.107",
    "polar-typescript": "^2.1.107"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.117.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.117.0",
    "@aws-cdk/core": "^1.117.0",
    "@types/node-fetch": "2.5.7",
    "firebase": "=8.8.1",
    "node-fetch": "=2.6.1",
    "source-map-support": "=0.5.19",
